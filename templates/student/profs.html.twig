{% extends 'base.html.twig' %}

{% block title %}Profs{% endblock %}

{% block body %}
    <h1>Profs de {{ app.user.name }}</h1>


    <table class="table">
        <thead>
        <tr>
            <th scope="col">Profs</th>
        </tr>
        </thead>
        <tbody>
        {% for pr in student.profs %}
            <tr>
                <td>{{ pr.name }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
